public class Teclado {
private String Número_Série;
fabricante privado de cordas;
String Cor privada;
public Teclado(String n , String f, String c) {
this.Numero_Serie=n;
this.Fabricante=f;
this.Cor=c;
}
public Teclado() {
this.Numero_Serie=nulo;
this.Fabricante=nulo;
this.Cor=nulo;
}
public void Imprime_Teclado() {
System.out.println("---- Função Imprime_Teclado() ------");
System.out.println("Número de Série do Teclado: " +
this.Numero_Serie);
System.out.println("Fabricante do Teclado: " + this.Fabricante);
System.out.println("Cor do Teclado: " + this.Cor);
}
public String getNumero_Serie() {
return Número_Série;
}
public void setNumero_Serie(String numero_Serie) {
Numero_Serie = numero_Serie;
}
public String getFabricante() {
voltar Fabricante;
}
public void setFabricante(String fabricante) {
Fabricante = fabricante;
}
public String getCor() {
retornar Cor;
}
public void setCor(String cor) {
Cor = cor;
}
}

