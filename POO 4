pacote usc;
public class Computador {
private String numero_Serie;
mouse privado;
teclado Teclado privado;
gabinete gabinete privado;
monitor monitor privado;
public String getNumero_Serie() {
return numero_Série;
}
public void setNumero_Serie(String numero_Serie) {
this.numero_Serie = numero_Serie;
}
public Rato getMouse() {
retornar mouse;
}
public void setMouse(Mouse mouse) {
this.mouse = mouse;
}
public Teclado getTeclado() {
teclado de retorno;
}
public void setTeclado(teclado Teclado) {
this.teclado = teclado;
}
public Gabinete getGabinete() {
retornar gabinete;
}
public void setGabinete(gabinete gabinete) {
this.gabinete = gabinete;
}
public Monitor getMonitor() {
monitor de retorno;
}
public void setMonitor(monitor monitor) {
this.monitor = monitor;
}

public Computador(String numero_Serie, Mouse mouse, Teclado teclado,
Gabinete gabinete, Monitor m) {
this.numero_Serie = numero_Serie;
this.mouse = mouse;
this.teclado = teclado;
this.gabinete = gabinete;
este.monitor = m;
}
public Computador() {
this.numero_Serie = nulo;
this.mouse = null;
this.teclado = null;
this.gabinete = null;
this.monitor = nulo;
}
public void Imprime_Computador () {
System.out.println("------Função Imprime_Computador() -------");
System.out.println("Número da Série do Computador: " +
this.numero_Serie);
this.gabinete.Imprime_Gabinete();
this.monitor.Imprime_Monitor();
this.teclado.Imprime_Teclado();
this.mouse.Imprime_Mouse();
}
}

